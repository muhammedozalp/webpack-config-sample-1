{
    "name": "@sample_workspaces/mobile",
    "version": "0.1.1",
    "description": "Front-end developments of sample mobile theme via webpack and twig ",
    "homepage": "https://gitlab.sample.com/company/sample/",
    "bugs": "https://sentry.sample.com/organizations/sample-dev",
    "repository": {
        "type": "git",
        "url": "https://gitlab.sample.com/companywp5-cdn-sass"
    },
    "private": true,
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "scripts": {
        "mobile-dev": "webpack --mode development --env no_debug --env no_report --env has_sourcemap",
        "mobile-dev-live": "webpack --mode development --env no_debug --env no_report --env has_sourcemap --env has_live",
        "mobile-dev-server": "node ./config/server.js --mode development --env no_debug --env no_report --env has_sourcemap",
        "mobile-dev-watch": "webpack --watch --mode development --env no_debug --env no_report --env isWatching --env has_sourcemap --stats-error-details",
        "mobile-dev-watch-eslint": "webpack --watch --mode development --env no_debug --env no_report --env isWatching --env has_sourcemap --stats-error-details --env eslint",
        "mobile-dev-live-watch": "webpack --watch --mode development --env no_debug --env no_report --env isWatching --env has_sourcemap --env has_live --stats-error-details",
        "mobile-prod": "webpack --mode production --env no_debug --env no_report --env no_sourcemap --stats-error-details",
        "mobile-prod-live": "webpack --mode production --env no_debug --env no_report --env no_sourcemap --env has_live --stats-error-details",
        "mobile-prod-via-sourcemap": "webpack --mode production --env no_debug --env no_report --env has_sourcemap --stats-error-details",
        "mobile-prod-watch": "webpack --watch --mode production --env no_debug --env no_report --env isWatching --env no_sourcemap --stats-error-details",
        "mobile-prod-live-watch": "webpack --watch --mode production --env no_debug --env no_report --env isWatching --env --env has_live no_sourcemap --stats-error-details",
        "section_debug": "___________________________________________________________________________________________________",
        "mobile-dev-debug": "webpack --mode development --env has_debug --env has_report --env has_sourcemap",
        "mobile-prod-debug": "webpack --mode production --env no_debug --env has_report --env no_sourcemap --stats-error-details",
        "mobile-prod-sourcemap": "webpack --mode production --env has_debug --env has_report --env has_sourcemap --stats-error-details",
        "mobile-prod-debug-stats": "webpack --mode production --env has_debug --env has_report --env no_sourcemap --profile --json ./bundle_analysis/stats.json",
        "mobile-prod-bundle-stats": "webpack --mode production --env has_debug --env no_sourcemap --env has_bundlestats",
        "mobile-whybundled": "whybundled ./bundle_analysis/stats.json",
        "mobile-whybundledby": "whybundled by ./bundle_analysis/stats.json",
        "section_doc_and_test": "____________________________________________________________________________________________",
        "mobile-doc": "rm -rf ./jsdocs/docs && jsdoc -c jsdoc.json",
        "mobile-test-eslint": "eslint --fix-dry-run ./src/template/**/*.js ./src/util/**/*.js --quiet",
        "mobile-test-stylelint": "stylelint src/**/*.{css,scss}",
        "mobile-test-webpackconfig": "webpack configtest ./webpack.config.js"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.2.1",
        "@googlemaps/js-api-loader": "^1.16.2",
        "@googlemaps/markerclusterer": "^2.3.1",
        "@popperjs/core": "^2.11.6",
        "@sentry/browser": "^6.18.1",
        "@sentry/tracing": "^6.18.1",
        "ajv": "^8.11.2",
        "axios": "^1.2.1",
        "bootstrap": "^5.2.3",
        "element-closest-polyfill": "^1.0.5",
        "fuse.js": "^6.6.2",
        "intl-tel-input": "^17.0.19",
        "process": "0.11.10",
        "raven-js": "3.27.2",
        "sweetalert2": "11.4.24",
        "swiper": "^8.4.5",
        "whatwg-fetch": "3.6.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.20.2",
        "@fullhuman/postcss-purgecss": "^5.0.0",
        "babel-loader": "^9.1.0",
        "babel-plugin-transform-require-context": "^0.1.1",
        "bundle-stats-webpack-plugin": "^4.1.6",
        "chalk": "4.1.2",
        "copy-webpack-plugin": "^11.0.0",
        "core-js": "^3.26.1",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "dotenv-webpack": "^8.0.1",
        "eslint": "^8.30.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-sonarjs": "^0.17.0",
        "eslint-webpack-plugin": "4.0.1",
        "express": "^4.18.2",
        "filemanager-webpack-plugin": "^8.0.0",
        "jsdoc": "^4.0.0",
        "mini-css-extract-plugin": "^2.7.2",
        "postcss": "^8.4.20",
        "postcss-loader": "^7.0.2",
        "postcss-preset-env": "^7.8.3",
        "regenerator-runtime": "^0.13.11",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",
        "stylelint": "^14.16.0",
        "stylelint-config-standard-scss": "^6.1.0",
        "stylelint-scss": "^4.3.0",
        "webpack": "^5.75.0",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-middleware": "^6.0.1",
        "webpack-manifest-plugin": "^5.0.0",
        "whybundled": "^2.0.0"
    },
    "browserslist": {
        "legacyBrowsers": [
            "and_chr 68"
        ],
        "modernBrowsers": [
            "last 2 versions"
        ]
    }
}
